<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@color/color_F7F8F8"
    android:orientation="vertical">

    <TextView
        android:id="@+id/tv_editor_info"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_24"
        android:background="@color/white"
        android:drawableRight="@drawable/right_arrow"
        android:drawablePadding="@dimen/dp_12"
        android:gravity="center_vertical"
        android:minHeight="@dimen/dp_50"
        android:paddingLeft="@dimen/dp_15"
        android:paddingRight="@dimen/dp_15"
        android:text="@string/editext_info"
        android:textColor="@color/text_color20_1D2023"
        android:textSize="@dimen/dp_16" />

    <View
        style="@style/HorizontalStyle"
        android:layout_marginLeft="@dimen/dp_15"
        android:layout_marginRight="@dimen/dp_15" />

    <TextView
        android:id="@+id/tv_attached_download_address"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_24"
        android:background="@color/white"
        android:drawableRight="@drawable/right_arrow"
        android:drawablePadding="@dimen/dp_12"
        android:gravity="center_vertical"
        android:minHeight="@dimen/dp_50"
        android:paddingLeft="@dimen/dp_15"
        android:paddingRight="@dimen/dp_15"
        android:text="@string/attached_download_address"
        android:textColor="@color/text_color20_1D2023"
        android:textSize="@dimen/dp_16" />

    <View
        style="@style/HorizontalStyle"
        android:layout_marginLeft="@dimen/dp_15"
        android:layout_marginRight="@dimen/dp_15" />

    <TextView
        android:id="@+id/tv_my_attachment"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:drawableRight="@drawable/right_arrow"
        android:drawablePadding="@dimen/dp_12"
        android:gravity="center_vertical"
        android:minHeight="@dimen/dp_50"
        android:paddingLeft="@dimen/dp_15"
        android:paddingRight="@dimen/dp_15"
        android:text="@string/my_attachment"
        android:textColor="@color/text_color20_1D2023"
        android:textSize="@dimen/dp_16" />

    <View
        style="@style/HorizontalStyle"
        android:layout_marginLeft="@dimen/dp_15"
        android:layout_marginRight="@dimen/dp_15" />

    <TextView
        android:id="@+id/tv_feedback"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_24"
        android:background="@color/white"
        android:drawableRight="@drawable/right_arrow"
        android:drawablePadding="@dimen/dp_12"
        android:gravity="center_vertical"
        android:minHeight="@dimen/dp_50"
        android:paddingLeft="@dimen/dp_15"
        android:paddingRight="@dimen/dp_15"
        android:text="@string/feedback"
        android:textColor="@color/text_color20_1D2023"
        android:textSize="@dimen/dp_16" />

    <View
        style="@style/HorizontalStyle"
        android:layout_marginLeft="@dimen/dp_15"
        android:layout_marginRight="@dimen/dp_15" />

    <TextView
        android:id="@+id/tv_about_us"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:drawableRight="@drawable/right_arrow"
        android:drawablePadding="@dimen/dp_12"
        android:gravity="center_vertical"
        android:minHeight="@dimen/dp_50"
        android:paddingLeft="@dimen/dp_15"
        android:paddingRight="@dimen/dp_15"
        android:text="@string/about_us"
        android:textColor="@color/text_color20_1D2023"
        android:textSize="@dimen/dp_16" />

    <View
        style="@style/HorizontalStyle"
        android:layout_marginLeft="@dimen/dp_15"
        android:layout_marginRight="@dimen/dp_15" />

    <TextView
        android:id="@+id/tv_privacy_and_policy"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:drawableRight="@drawable/right_arrow"
        android:drawablePadding="@dimen/dp_12"
        android:gravity="center_vertical"
        android:minHeight="@dimen/dp_50"
        android:paddingLeft="@dimen/dp_15"
        android:paddingRight="@dimen/dp_15"
        android:text="@string/privacy_and_policy"
        android:textColor="@color/text_color20_1D2023"
        android:textSize="@dimen/dp_16" />

    <View
        style="@style/HorizontalStyle"
        android:layout_marginLeft="@dimen/dp_15"
        android:layout_marginRight="@dimen/dp_15" />
<LinearLayout
    android:layout_width="match_parent"
    android:orientation="horizontal"
    android:layout_height="wrap_content">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:drawablePadding="@dimen/dp_12"
        android:gravity="center_vertical"
        android:minHeight="@dimen/dp_50"
        android:paddingLeft="@dimen/dp_15"
        android:paddingRight="@dimen/dp_15"
        android:text="@string/version_information"
        android:textColor="@color/text_color20_1D2023"
        android:textSize="@dimen/dp_16" />

    <TextView
        android:id="@+id/tv_version_information"
        android:layout_width="0dp"
        android:layout_weight="1"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:drawableRight="@drawable/right_arrow"
        android:drawablePadding="@dimen/dp_12"
        android:gravity="center_vertical|right"
        android:minHeight="@dimen/dp_50"
        android:paddingLeft="@dimen/dp_15"
        android:paddingRight="@dimen/dp_15"
        tools:text="v1.1.1"
        android:textColor="@color/text_color20_1D2023"
        android:textSize="@dimen/dp_16" />
</LinearLayout>


    <View
        style="@style/HorizontalStyle"
        android:layout_marginLeft="@dimen/dp_15"
        android:layout_marginRight="@dimen/dp_15" />

    <TextView
        android:id="@+id/tv_exit"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_24"
        android:background="@color/white"
        android:drawablePadding="@dimen/dp_12"
        android:gravity="center"
        android:minHeight="@dimen/dp_50"
        android:text="@string/exit"
        android:textColor="@color/text_color_E50120"
        android:textSize="@dimen/dp_16" />
</LinearLayout>
